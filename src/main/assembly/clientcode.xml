<assembly>
  <id>clientcode</id>
  <includeBaseDirectory>false</includeBaseDirectory>
  <formats>
    <format>zip</format>
  </formats>
  <fileSets>
  	<fileSet>
  		<directory>target/WorklistProto-0.1-SNAPSHOT</directory>
  		<includes>
 			<include>**/*.js</include>
  			<include>**/*.png</include>
 			<include>**/*.rpc</include>
 			<include>**/*.html</include>
 			<include>**/*.gif</include>
