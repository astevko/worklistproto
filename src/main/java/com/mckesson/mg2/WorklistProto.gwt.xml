<?xml version="1.0" encoding="UTF-8"?>
<!--
  When updating your version of GWT, you should also update this DTD reference,
  so that your app can take advantage of the latest GWT module capabilities.
-->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
  "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to='worklistproto'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.debug.Debug"/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->

  <!-- Other module inherits                                      -->
  <inherits name="com.vaadin.polymer.Elements"/>
  <!-- you want to use gwt phonegap -->
  <inherits name="com.googlecode.gwtphonegap.PhoneGap"/>
	
<!-- <set-configuration-property name="CssResource.style" value="pretty"/> 
 -->	
	<!-- set the user agent to safari -> only webkit -->
	<set-property name="user.agent" value="safari" />
	
	<!-- as per https://code.google.com/p/gwt-phonegap/wiki/LoggingModule -->
	<set-property name="gwt.logging.enabled" value="FALSE" />
	<set-property name="phonegap.logging.consoleHandler" value="DISABLED" />
	<set-property name="phonegap.logging.remoteHandler" value="DISABLED" />
	
	<!-- This can be used to make CssResource produce human-readable CSS -->
<!-- 	<define-configuration-property name="CssResource.style" is-multi-valued="false" />
	<set-configuration-property name="CssResource.style" value="obf" />	
 -->	
  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.mckesson.mg2.client.WorklistProto'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>

</module>
